{
    "name": "smartassert/health-check-bundle",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/smartassert/HealthCheckBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SmartAssert\\HealthCheckBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SmartAssert\\HealthCheckBundle\\Tests\\": ""
        }
    },
    "scripts": {
        "cs-fixer-dry-run": "./vendor/bin/php-cs-fixer check --allow-risky=yes --verbose --show-progress none",
        "cs-fixer-fix": "./vendor/bin/php-cs-fixer fix --allow-risky=yes --diff",
        "cs": [
            "@cs-fixer-dry-run",
            "@cs-phpcs"
        ],
        "static-analysis": "./vendor/bin/phpstan analyse --ansi",
        "test": "./vendor/bin/phpunit --colors=always",
        "test-ci": [
            "APP_ENV=test ./vendor/bin/phpunit --colors=always",
            "APP_ENV=test_readiness_inspector_enabled_and_false ./vendor/bin/phpunit --colors=always",
            "APP_ENV=test_readiness_inspector_enabled_and_true ./vendor/bin/phpunit --colors=always",
            "APP_ENV=test_version_inspector_enabled ./vendor/bin/phpunit --colors=always"
        ],
        "ci": [
            "@cs",
            "@static-analysis",
            "@test"
        ]
    },
    "require": {
        "php": "^8.4",
        "ext-json": "*",
        "doctrine/orm": "^3",
        "smartassert/doctrine-inspectors": "^4.0",
        "smartassert/invokable-logger": "^0.3.0",
        "smartassert/service-status-inspector": "^6.0",
        "symfony/config": "^7 || ^8",
        "symfony/dependency-injection": "^7 || ^8",
        "symfony/http-foundation": "^7 || ^8",
        "symfony/http-kernel": "^7 || ^8",
        "symfony/yaml": "^7 || ^8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "mockery/mockery": "^1.5",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^12",
        "symfony/browser-kit": "^7 || ^8",
        "symfony/framework-bundle": "^7 || ^8",
        "symfony/monolog-bundle": "^3.7",
        "symfony/routing": "^7 || ^8",
        "webignition/object-reflector": "^1.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
